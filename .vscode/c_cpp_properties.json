{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/include",
                "${workspaceFolder}/include/",
                "${workspaceFolder}/include/network",
                "${workspaceFolder}/include/error",
                "${workspaceFolder}/include/tests",
                "${workspaceFolder}/include/misc",
                "${workspaceFolder}/include/models",
                "${workspaceFolder}/include/cserver"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": false
            },
            "defines": [
                "_POSIX_C_SOURCE=200112L"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}